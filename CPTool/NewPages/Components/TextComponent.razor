@typeparam T

@if (Disable)
{
    <FieldComponent Label=@Label Value=@StringValue></FieldComponent>
}
else
{
    <MudTextField T="string" Value="@StringValue"
              Immediate="true"
              Disabled="@Disable"
              ValueChanged="OnValueChanged"
              Margin="Margin.Dense"
              Variant="Variant.Outlined"
              Required=@Required
              RequiredError="@RequiredError"
              Validation="Validation"
              InputMode=@InputMode
              InputType="@InputType"
              Mask="@Mask"
              Lines="@Lines"
              Label="@Label" />
}


@code {

}
