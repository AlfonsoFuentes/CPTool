@page "/mainsettings/{TableId:int}"
@using CPTool.Application.Features.MaterialFeatures.Command.CreateEdit
@using CPTool.Application.Features.MaterialFeatures.Command.Delete
@using CPTool.Application.Features.MaterialFeatures.Query.GetById
@using CPTool.Application.Features.MaterialFeatures.Query.GetList
@using CPTool.NewPages.Dialogs.BrandSupplier.List


@switch (TableId)
{
    case 1:
        {

            <SimpleListComponent TMaster="AddEditMWOTypeCommand"
                     TDeleteMaster="DeleteMWOTypeCommand"
                     TMasterList="GetMWOTypeListQuery"
                     TGedById="GetByIdMWOTypeQuery"
                     OnShowDialogMaster="ToolDialogService.ShowNameDialog">



            </SimpleListComponent>

        }
        break;
    case 2:
        {
            <SimpleListComponent TMaster="AddEditUnitaryBasePrizeCommand"
                     TDeleteMaster="DeleteUnitaryBasePrizeCommand"
                     TMasterList="GetUnitaryBasePrizeListQuery"
                     TGedById="GetByIdUnitaryBasePrizeQuery"
                     OnShowDialogMaster="ToolDialogService.ShowNameDialog">
            </SimpleListComponent>


        }
        break;
    case 3:
        {
            <SimpleListComponent TMaster="AddEditGasketCommand"
                     TDeleteMaster="DeleteGasketCommand"
                     TMasterList="GetGasketListQuery" TGedById="GetByIdGasketQuery"
                     OnShowDialogMaster="ToolDialogService.ShowNameDialog">
            </SimpleListComponent>

        }
        break;
    case 4:
        {
            <SimpleListComponent TMaster="AddEditMaterialCommand"
                     TDeleteMaster="DeleteMaterialCommand"
                     TMasterList="GetMaterialListQuery"
                     TGedById="GetByIdMaterialQuery"
                     OnShowDialogMaster="ToolDialogService.ShowNameDialog">
            </SimpleListComponent>

        }
        break;
    case 5:
        {
            <EquipmentTypeList></EquipmentTypeList>
        }
        break;
        case 6:
        {
           <BrandSupplierList></BrandSupplierList>
        }
        break;
    @* case 7:
        {
            <PurchaseOrderMWOItemList OnShowDialogDetail=ToolDialogService.ShowPurchaseOrderItemDialog
                          OnShowDialogMaster=ToolDialogService.ShowPurchaseOrderDialog>

            </PurchaseOrderMWOItemList>

        }
        break;
    case 8:
        {

            <DownPaymenList></DownPaymenList>


        }
        break;*@
}
@code {
    [Parameter]
    public int TableId { get; set; } = 0;

}
